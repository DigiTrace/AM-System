{% extends 'base.html.twig' %}
{% form_theme addform 'bootstrap_3_layout.html.twig' %} 
{% block content1 %}
            
    {% for label, messages in app.flashes %}
         {% for message in messages %}
             <div class="alert alert-{{ label }}">
                 {{ message | trans | nl2br}}
             </div>
         {% endfor %}
     {% endfor %}
     
   <h2>{% trans %}add_case_headline{% endtrans %}</h2>
    {{ form_start(addform) }}
